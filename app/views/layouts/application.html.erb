<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Samplace</title>
  <%= stylesheet_link_tag "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
    <% if flash[:notice] %>
        <div class="notice">
          <div class="notice-wrap"><%= flash[:notice] %></div>
        </div>
    <% end %>
    <div class="wrap">
      <%= render 'layouts/header' %>
      <%= yield %>
    </div>
    <%= render 'layouts/footer' %>
</body>
</html>
